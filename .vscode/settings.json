{
  "workbench.colorCustomizations": {
    "titleBar.activeBackground": "#3cae7b40",
    "tab.activeBackground": "#1d1994b1",
    "titleBar.inactiveBackground": "#3cae7b40"
  },
  "i18n-ally.localesPaths": ["**/locales/lang", "**/src/views/*/lang"],
  // "i18n-ally.localesPaths": ["src/views/*/lang"],
  "i18n-ally.keystyle": "nested",
  "i18n-ally.sortKeys": true,
  "i18n-ally.namespace": true,
  "i18n-ally.sourceLanguage": "en",
  "i18n-ally.displayLanguage": "zh-CN",
  // 建议使用json文件，因为可以使用插件读写，单文件能让插件更好的运行，建议小项目使用
  "i18n-ally.enabledParsers": ["json"],
  "i18n-ally.enabledFrameworks": ["vue", "react"]
}
